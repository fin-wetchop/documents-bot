    "build:web-server": "npx ebi build -b ./src/microservices/web-server/build --entry ./src/microservices/web-server/src/index.ts",
    "run:telegram-bot": "node ./src/microservices/telegram-bot",
    "run:migration": "node ./src/microservices/migration",
    "run:documents": "node ./src/microservices/documents",
    "run:web-server": "node ./src/microservices/web-server"
