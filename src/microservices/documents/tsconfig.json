{
  "compilerOptions": {
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "strictPropertyInitialization": false
  }
}
